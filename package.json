{
  "name": "zenodeck-user-service",
  "version": "1.0.0",
  "scripts": {
    "db:clean": "tsx --env-file=.env src/db/clean.ts",
    "db:seed": "tsx --env-file=.env src/db/seed.ts",
    "deploy": "serverless zenodeck-user-service deploy --stage dev",
    "dev:user": "serverless zenodeck-user-service:offline --stage dev",
    "env": "ln .env $PWD/services/user/.env",
    "format": "prettier --write .",
    "package": "serverless zenodeck-user-service package --stage dev"
  },
  "devDependencies": {
    "@serverless/compose": "^1.3.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.5",
    "@types/serverless": "^3.12.22",
    "esbuild": "^0.24.0",
    "prettier": "^3.3.3",
    "serverless": "^3.40.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.54.6",
    "serverless-offline": "^13.8.3",
    "serverless-prune-plugin": "^2.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/s3-presigned-post": "^3.654.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@middy/core": "^5.4.7",
    "@middy/do-not-wait-for-empty-event-loop": "^5.4.7",
    "@middy/http-error-handler": "^5.4.7",
    "@middy/http-json-body-parser": "^5.4.7",
    "@middy/util": "^5.4.7",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "dayjs": "^1.11.13",
    "json-schema-to-ts": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "serverless-plugin-warmup": "^8.3.0"
  }
}
